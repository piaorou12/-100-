//1.
#include<stdio.h>
int main()
{
    char str[100];
    scanf("%s",str);
    printf("Hello World %s!\n",str);

    return 0;

}
--------------------------------------------------
//3.
#include<stdio.h>
 void TurnFtoC(double F)
{
    double C = 5 * (F - 32) /9;
    printf("%.5lf",C);
}
int main()
{
    double F;
    scanf("%lf",&F);
    TurnFtoC(F);

    return 0;

}
--------------------------------------------------

